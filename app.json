{
  "expo": {
    "name": "phoenix",
    "slug": "phoenix",
    "scheme": "phoenix",
    "deepLinking": true,
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-font", "expo-notifications", "expo-router"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.kanebernard.phoenix",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.kanebernard.phoenix",
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
